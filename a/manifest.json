{
  "name": "test",
  "description": "test",
  "manifest_version": 2,
  "version": "1.0.0",
  "permissions": [
    "tabs",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "commands": {
    "filter-switch": {
      "suggested_key": {
        "default": "Ctrl+P",
        "mac": "MacCtrl+P"
      },
      "description": "全局开关"
    },
    "filter-only-list": {
      "suggested_key": {
        "default": "Ctrl+O",
        "mac": "MacCtrl+O"
      },
      "description": "只使用黑白名单开关"
    }
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}
